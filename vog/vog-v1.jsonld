{
  "@context": {
    "@version": 1.1,
    "@protected": true,

    "vog": "https://sphereon-opensource.github.io/vc-contexts/vog/vog-v1#",
    "schema": "https://schema.org/",

    "e-vog": {
      "@id": "https://sphereon-opensource.github.io/vc-contexts/vog/vog-v1#e-vog",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",



        "afgiftedatum": {
          "@id": "vog:afgiftedatum",
          "@type": "schema:Date"
        },
        "onsKenmerk": "vog:onsKenmerk",
        "geslachtsnaam": "vog:geslachtsnaam",
        "tussenvoegsels": "vog:tussenvoegsels",
        "voornamen": "vog:voornamen",
        "geboortedatum": {
          "@id": "vog:geboortedatum",
          "@type": "schema:Date"
        },
        "geboorteplaats": "vog:geboorteplaats",
        "geboorteland": "vog:geboorteland",
        "functie": "vog:functie",
        "doel": "vog:doel",
        "belanghebbendeNaam": "vog:belanghebbendeNaam",
        "functieAspecten": {
          "@id": "vog:functieAspecten",
          "@container": "@list",
          "@type": "vog:functieAspect"
        },
        "screeningsProfielOmschrijving": "vog:screeningsProfielOmschrijving"
      }
    },

    "doel": {
      "@id": "https://sphereon-opensource.github.io/vc-contexts/vog/vog-v1#doel",


      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "doelType": "vog:doelType",
        "doelOmschrijving": "vog:doelOmschrijving"
      }
    },

    "functieAspect": {
      "@id": "https://sphereon-opensource.github.io/vc-contexts/vog/vog-v1#functieAspect",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "nr": {
          "@id": "vog:functieAspectNr",
          "@type" : "schema:Number"
        },
        "omschrijving": "vog:functieAspectOmschrijving"
      }
    }
  }
}
